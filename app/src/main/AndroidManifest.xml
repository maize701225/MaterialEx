<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="idv.hangermo.materialex"
    android:screenOrientation="portrait" >


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".lastest.Lastest"
            android:label="@string/newest" >
        </activity>
        <activity
            android:name=".hotnews.HotNews"
            android:label="@string/hotnews" >
        </activity>
        <activity
            android:name=".lazyroute.LazyRoute"
            android:label="@string/lazyroute" >
        </activity>

        <activity
            android:name=".lazyroute.LazyRouteTownship"
            android:label="@string/LazyRouteTownship" >
        </activity>

        <activity
            android:name=".lazyroute.LazyRouteYilan1D"
            android:label="@string/LazyRouteYilan1D" >
        </activity>

        <activity
            android:name=".lazyroute.LazyRouteCingjing"
            android:label="@string/LazyRouteCingjing" >
        </activity>

        <activity
            android:name=".dateplace.DatePlace"
            android:label="@string/dateplace" >
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="@string/google_maps_key" />

    </application>

</manifest>
